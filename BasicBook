public class BasicBook {
    private String title;
    private String author;
    public int ISBN;
    public int copies;
    public String description;

    public BasicBook (String description, String title, String author, int ISBN, Boolean ifBorrowed, int copies){
        this.description = description;
        this.title = title;
        this.author = author;
        this.ISBN = ISBN;
        this.copies = copies;
    }
    public String getTitle () {
        return title;
    }

    public String getAuthor () {
        return author;
    }

    public int getISBN () {
        return ISBN;
    }

    public String getDescription () {
        return description;
    }

    public int getCopies() {
        return copies;
        }

    }


